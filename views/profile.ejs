<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Document</title>
</head>
<style>
  body{background: skyblue;font: sans-serif;color: royalblue;padding: 30px;}
  h1{font-size: 32px;text-align: center;text-transform: uppercase;letter-spacing: 2px;}
  h2{font-size: 28px;text-align: center;text-transform: uppercase;letter-spacing: 2px;}
  p{font-size: 16px;text-align: center}
  li{font-size: 16px;text-align: center}
</style>
<body>
  <% include partials/nav.ejs %>
  <h1>Welcome to the profile <%=person%></h1>
  <p>
    <strong>Age: </strong><%=data.age%>
  </p>
  <p>
      <strong>Job: </strong><%=data.job%>
  </p>
  <h2>Hobbies</h2>
  <ul>
  <% data.hobbies.forEach(function(items){ %>
    <li><%= items %></li>
  <% }); %>
  </ul>
</body>
</html>
